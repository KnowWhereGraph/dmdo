###  https://knowwheregraph.org/undrr-hip/H0100101
undrr-hip:H0100101 rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty undrr-hip:broader ;
                                     owl:someValuesFrom undrr-hip:H001
                                   ] ,
